{
  "manifest_version": 2,
  "name": "Replace With My Face",
  "version": "1.0",
  "web_accessible_resources": [
    "faces/*.jpg"
  ],
  "description": "a simple chrome extension that replaces all images into my face!",
  "content_scripts": [{
      "matches": [
        "<all_urls>"
      ],
      "js":["face.js"]
    }],
  "browser_action":{
    "default_icon":"icon.png"
  }
}
